public class PrintStar{
  public static void main(String[] args){
   
    for(int x = 1; x <= 16; x*=2){
      System.out.println();
      for (int y = 0; y < x; y++){
        System.out.print("*");
        
      }   
    }
    System.out.println("\n");
    int a = 0, b = 0;
    
    while (a < 2){
      while (b < 3){
      System.out.print("*");
      b++;
      }
     a++;
     System.out.println();
     b = 0;
    }
       
  
  
  }
  }
